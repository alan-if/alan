#!/bin/sh
for f in *.output ; do
    case=`basename $f .output`
        if [ -f $case`cat .case` ] ; then
	        /bin/echo -n $case
            if [ -f $case.expected ] ; then
                if [ -f $case.suspended ] ; then
	                /bin/echo " (but suspended)"
                else
                    /bin/echo
                fi
            else
                if [ -f $case.suspended ] ; then
	                /bin/echo " (suspended)"
                else
                    /bin/echo " (pending)"
                fi
            fi
        fi
done

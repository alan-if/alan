#++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
#
# Makefile settings for compiling Alan compiler on MacOSX
#

OSFLAGS = -m32 -mmacosx-version-min=10.5

COMPILER = gcc
EXTRA_COMPILER_FLAGS = -g -Wall -funsigned-char

LINKER = gcc
EXTRA_LINKER_FLAGS = 

CGREEN = yes
CGREENROOT = /usr/local
CGREENDIR = $(CGREENROOT)/include/cgreen
CGREENLIB = -lcgreen

release: EXTRA_COMPILER_FLAGS = -O4

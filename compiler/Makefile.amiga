# Makefile for alan on an Amiga with AztecC
#


CFLAGS	= -n +l +d $(CQ)
LDFLAGS	= -lcl32 -lml -g $(LQ)

OBJECTS = \
	Adventure.o \
	Alternative.o \
	Attribute.o \
	Check.o \
	Class.o \
	Container.o \
	Does.o \
	Element.o \
	Event.o \
	Exit.o \
	Expression.o \
	Id.o \
	Instance.o \
	Limit.o \
	List.o \
	Option.o \
	Restriction.o \
	Rule.o \
	Score.o \
	Script.o \

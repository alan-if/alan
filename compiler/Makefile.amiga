	Statement.o \
	Step.o \
	Symbol.o \
	Synonym.o \
	Syntax.o \
	Verb.o \
	What.o \
	Where.o \
	dump.o \
	encode.o \
	emit.o \
	timing.o \
	spa.o \
	alan.o \
	pmPaSema.o pmParse.o pmErr.o \
	smScan.o smScSema.o \
	lmList.o

#-- MAIN TARGET
alan: $(OBJECTS) sysdep.o version.o
	ln -o $@ -g $(OBJECTS) sysdep.o version.o $(LDFLAGS)
	copy alan /

pmPaSema.o: pmPaSema.c
	cc $(CFLAGS) -y500 -e200 pmPaSema.c

lmList.o: lmList.c
	cc $(CFLAGS) -z5000 lmList.c

emit.o: emit.c
	cc $(CFLAGS) -DWORDADDRESS -L400 emit.c



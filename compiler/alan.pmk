%%INSERTSYMBOL

    /* Make the requested token */
    sym->code = code;
    strcpy(sym->chars, sstr);


%%DECLARATIONS

#ifdef _PROTOTYPES_
static int val(char str[])
#else
static int val(str)
     char str[];
#endif
{
  int temp;

  sscanf(str, "%d", &temp);
  return(temp);
}



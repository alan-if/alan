attributes		-- RETURNS: atrs
    = is attribute_list
    %% %attributes.atrs = attribute_list.atrs; %%

    ! attributes is attribute_list
    %% %attributes.atrs = concat(%attribute_list.atrs, %attributes.atr); %%
    ;

attribute_list		-- RETURNS: atrs
    = attribute '.'
    %% %attribute_list.atrs = concat(NULL, %attribute.atr); %%

    ! attribute_list attribute '.'
    %% %attribute_list.atrs = concat(%attribute_list.atrs, %attribute.atr); %%
    ;

attribute		-- RETURNS: atr
    = ID
    %%
	%attribute.atr = newatr(`&%ID.srcp,
				TYPBOOL,
				newnam(`&%ID.srcp, %ID.chars),
				TRUE, 0, 0);
    %%

    ! 'NOT' ID
    %%
	%attribute.atr = newatr(`&%ID.srcp,
				TYPBOOL,
				newnam(`&%ID.srcp, %ID.chars),
				FALSE, 0, 0);
    %%

    ! ID optional_minus INTEGER
    %%
	if (%optional_minus.minus)
		%attribute.atr = newatr(`&%ID.srcp,
					TYPINT,
					newnam(`&%ID.srcp, %ID.chars),
					-val(%INTEGER.chars), 0, 0);
	else
		%attribute.atr = newatr(`&%ID.srcp,
					TYPINT,
					newnam(`&%ID.srcp, %ID.chars),
					val(%INTEGER.chars), 0, 0);
    %%

    ! ID STRING	
    %%
	%attribute.atr = newatr(`&%ID.srcp,
				TYPSTR,
				newnam(`&%ID.srcp, %ID.chars),
				0, %STRING.fpos, %STRING.len);
    %%
    ;

is			-- RETURNS: srcp
    = 'IS'
    %% %is.srcp = %'IS'.srcp; %%

    ! 'ARE'
    %% %is.srcp = %'ARE'.srcp; %%

    ! 'HAS'
    %% %is.srcp = %'HAS'.srcp; %%
    ;


---> CLASS
class			-- RETURNS: cla
    = 'EVERY' ID optional_heritage slots class_tail
    %%{
	NamNod *nam = newnam(`&%ID.srcp, %ID.chars);

	%class.cla = newcla(`&%'EVERY'.srcp,
				nam,
				%optional_heritage.nams,
				%slots.nams,
				%slots.whr,
				%slots.cnt,
				%slots.surr,
				%slots.descr,
				%slots.ment,
				%slots.dos,
				%slots.exts,
				%slots.vrbs,
				%slots.scrs);
	if (%class_tail.nam != NULL) { /* END-id given */
	    if (!eqnams(nam, %class_tail.nam))
		lmLog(`&%class_tail.nam->srcp, 201, sevWAR, nam->str);
    }%%
    ;

class_tail		-- RETURNS: nam
    = 'END' 'EVERY' optional_id '.'
    %% %class_tail.nam = %optional_id.nam; %%
    ;


---> INSTANCE
instance			-- RETURNS: ins
    = 'THE' ID heritage slots instance_tail
    %%{
	NamNod *nam = newnam(`&%ID.srcp, %ID.chars);

	%instance.ins = newins(`&%'THE'.srcp,
				nam,
				%heritage.nams,
				%slots.nams,
				%slots.whr,
				%slots.cnt,
				%slots.surr,
				%slots.descr,
				%slots.ment,
				%slots.dos,
				%slots.exts,
				%slots.vrbs,
				%slots.scrs);
	if (!eqnams(nam, %instance_tail.nam))
		lmLog(`&%instance_tail.nam->srcp, 201, sevWAR, nam->str);
    }%%
    ;

instance_tail		-- RETURNS: nam
    = 'END' 'THE' optional_id '.'
    %% %instance_tail.nam = %optional_id.nam; %%
    ;

optional_heritage
    =
    %% %optional_heritage.nams = NULL; %%

    ! heritage
    %% %optional_heritage.nams = %heritage.nams; %%
    ;

heritage
    = 'ISA' id_list
    %% %heritage.nams = %id_list.nams; %%
    ;


slots			-- RETURNS: nams, whr, atrs, cnt, 

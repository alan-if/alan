	%optional_exit_body.chks = %optional_checks.chks;
	%optional_exit_body.stms = %optional_does.stms;
	if (%optional_does.qual != QUAL_DEFAULT)
	     lmLog(`&%optional_does.srcp, 210, sevERR, "");

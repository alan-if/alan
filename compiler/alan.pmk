            /* CONTAINER Already declared */
	    lmLogv(`&%<object_body_part>.srcp, 204, sevERR, "CONTAINER", "object", NULL);
          else
	    %<object_body>.cnt = %<object_body_part>.cnt;
        else if (%<object_body_part>.atrs != NULL)
          %<object_body>.atrs = combine(%<object_body>.atrs, %<object_body_part>.atrs);
	else if (%<object_body_part>.dscr != NULL)
          if (%<object_body>.dscr != NULL)
            /* DESCRIPTION Already declared */
	    lmLogv(`&%<object_body_part>.srcp, 204, sevERR, "DESCRIPTION", "object", NULL);
	  else
	    %<object_body>.dscr = %<object_body_part>.dscr;
        else if (%<object_body_part>.ment != NULL)
          if (%<object_body>.ment != NULL)
            /* MENTIONED Already declared */
	    lmLogv(`&%<object_body_part>.srcp, 204, sevERR, "MENTIONED", "object", NULL);
          else
	    %<object_body>.ment = %<object_body_part>.ment;
        else if (%<object_body_part>.art != NULL)
          if (%<object_body>.art != NULL)
            /* ARTICLE Already declared */
	    lmLogv(`&%<object_body_part>.srcp, 204, sevERR, "ARTICLE", "object", NULL);
          else
	    %<object_body>.art = %<object_body_part>.art;

%%TERMINALS

    Identifier = 2,1,1 => 'Identifier';

%%RECOVERY

    Fiducial('VERB', 'DOES', 'EXIT', 'ACTOR', 'EVENT', 'START', 'OBJECT',
		'SYNTAX', 'SCRIPT', 'LOCATION', 'SYNONYMS', 'CONTAINER',
		'MENTIONED', 'ATTRIBUTES', 'DESCRIPTION', 'END', 'CHECK');

    Separator(',');

    Skip(Identifier, Integer, '.');

%%RULES

--> ADVENTURE
<adventure>
    = <optional_options> <units> <start>
    ;

---> OPTIONS
<optional_options>	-- RETURNS: <nothing>
    =
    ! 'OPTIONS' <options>			%+('MESSAGE', 'DEFAULT')
    ;

<options>			-- RETURNS: <nothing>
    = <option>
    ! <options> <option>
    ;

<option>			-- RETURNS: <nothing>
    = ID '.'
    %%
	optBool(%ID.id->str, `&%ID.id->srcp, TRUE);
    %%

    ! ID ID '.'
    %%
	optenum(%ID.id->str, `&%ID.id->srcp, %2ID.id->str);
    %%

    ! ID Integer '.'
    %%
	optint(%ID.id->str, `&%ID.id->srcp, val(%Integer.chars));
    %%
    ;

---> UNITS
<units>
    = <unit>
    ! <units> <unit>
    ;

<unit>
    = <default>
    %% adv.atrs = combine(adv.atrs, %<default>.atrs); %%

    ! <object_default>
    %% adv.oatrs = combine(adv.oatrs, %<object_default>.atrs); %%

    ! <location_default>
    %% adv.latrs = combine(adv.latrs, %<location_default>.atrs); %%

    ! <actor_default>
    %% adv.aatrs = combine(adv.aatrs, %<actor_default>.atrs); %%

    ! <synonyms>
    %% adv.syns = combine(adv.syns, %<synonyms>.syns); %%

    ! <messages>
    %% adv.msgs = combine(adv.msgs, %<messages>.msgs); %%

    ! <syntax>
    %% adv.stxs = combine(adv.stxs, %<syntax>.stxs); %%

    ! <verb>
    %% adv.vrbs = concat(adv.vrbs, %<verb>.vrb, VRBNOD); %%

    ! <class>
    %% adv.clas = concat(adv.clas, %<class>.cla, CLANOD); %%
	
    ! <instance>
    %% adv.inss = concat(adv.inss, %<instance>.ins, INSNOD); %%

    ! <location>
    %% adv.locs = concat(adv.locs, %<location>.loc, LOCNOD); %%

    ! <object>
    %% adv.objs = concat(adv.objs, %<object>.obj, OBJNOD); %%

    ! <event>
    %% adv.evts = concat(adv.evts, %<event>.evt, EVTNOD); %%

    ! <container>
    %% adv.cnts = concat(adv.cnts, %<container>.cnt, CNTNOD); %%

    ! <actor>
    %% adv.acts = concat(adv.acts, %<actor>.act, ACTNOD); %%

    ! <rule>
    %% adv.ruls = concat(adv.ruls, %<rule>.rul, RULNOD); %%
    ;

----> DEFAULTS
<default>				-- RETURNS: atrs
    = 'DEFAULT' 'ATTRIBUTES' <attributes>		%+('MESSAGE', 'DEFAULT')
    %%
	%<default>.atrs = %<attributes>.atrs;
    %%
    ;

<location_default>		-- RETURNS: atrs
    = 'LOCATION' 'ATTRIBUTES' <attributes>		%+('MESSAGE', 'DEFAULT')
    %%
	%<location_default>.atrs = %<attributes>.atrs;
    %%
    ;

<object_default>			-- RETURNS: atrs
    = 'OBJECT' 'ATTRIBUTES' <attributes>			%+('MESSAGE', 'DEFAULT')
    %%
	%<object_default>.atrs = %<attributes>.atrs;
    %%
    ;

<actor_default>			-- RETURNS: atrs
    = 'ACTOR' 'ATTRIBUTES' <attributes>			%+('MESSAGE', 'DEFAULT')
    %%
	%<actor_default>.atrs = %<attributes>.atrs;
    %%
    ;

----> ATTRIBUTES
<attributes>		-- RETURNS: atrs
    = <attribute> '.'
    %%
	%<attributes>.atrs = concat(NULL, %<attribute>.atr, ATRNOD);
    %%

    ! <attributes> <attribute> '.'
    %%
	%<attributes>.atrs = concat(%<attributes>.atrs, %<attribute>.atr, ATRNOD);
    %%
    ;

<attribute>		-- RETURNS: atr
    = ID
    %%
	%<attribute>.atr = newatr(`&%ID.id->srcp,
				TYPBOOL,
				%ID.id,
				TRUE, 0, 0);
    %%

    ! 'NOT' ID
    %%
	%<attribute>.atr = newatr(`&%ID.id->srcp,
				TYPBOOL,
				%ID.id,
				FALSE, 0, 0);
    %%

    ! ID <optional_minus> Integer
    %%
	if (%<optional_minus>.minus)
		%<attribute>.atr = newatr(`&%ID.id->srcp,
					TYPINT,
					%ID.id,
					-val(%Integer.chars), 0, 0);
	else
		%<attribute>.atr = newatr(`&%ID.id->srcp,
					TYPINT,
					%ID.id,
					val(%Integer.chars), 0, 0);
    %%

    ! ID STRING	
    %%
	%<attribute>.atr = newatr(`&%ID.id->srcp,
				TYPSTR,
				%ID.id,
				0, %STRING.fpos, %STRING.len);
    %%
    ;

---> <SYNONYMS>
<synonyms>			-- RETURNS: syns
    = 'SYNONYMS' <synonym_list>	       			%+('MESSAGE', 'DEFAULT')
    %%
	%<synonyms>.syns = %<synonym_list>.syns;
    %%
    ;

<synonym_list>			-- RETURNS: syns
    = <synonym>
    %%
	%<synonym_list>.syns = concat(NULL, %<synonym>.syn, SYNNOD);
    %%

    ! <synonym_list> <synonym>
    %%
	%<synonym_list>.syns = concat(%<synonym_list>.syns, %<synonym>.syn, SYNNOD);
    %%
    ;

<synonym>				-- RETURNS: syn
    = <id_list> '=' ID '.'
    %%
	%<synonym>.syn = newsyn(`&%'='.srcp,
			      %<id_list>.idList,
			      %ID.id);
    %%
    ;

---> <MESSAGES>
<messages>			-- RETURNS: msgs
    = 'MESSAGE' <message_list>				%+('MESSAGE', 'DEFAULT')
    %%
	%<messages>.msgs = %<message_list>.msgs;
    %%
    ;

<message_list>			-- RETURNS: msgs
    = <message>
    %%
	%<message_list>.msgs = concat(NULL, %<message>.msg, MSGNOD);
    %%

    ! <message_list> <message>
    %%
	%<message_list>.msgs = concat(%<message_list>.msgs, %<message>.msg, MSGNOD);
    %%
    ;

<message>				-- RETURNS: msg
    = ID ':' <statements>			%-('QUIT', 'SAVE', 'RESTART', 'RESTORE')
    %%
	%<message>.msg = newmsg(`&%':'.srcp,
			      %ID.id,
			      %<statements>.stms);
    %%
    ;

---> <SYNTAX>				-- RETURNS: stxs
<syntax>
    = 'SYNTAX' <syntax_list>			%+('MESSAGE', 'DEFAULT')
    %%
	%<syntax>.stxs = %<syntax_list>.stxs;
    %%
    ;

<syntax_list>
    = <syntax_item>
    %%
	%<syntax_list>.stxs = concat(NULL, %<syntax_item>.stx, STXNOD);
    %%

    ! <syntax_list> <syntax_item>
    %%
	%<syntax_list>.stxs = concat(%<syntax_list>.stxs, %<syntax_item>.stx, STXNOD);
    %%
    ;

<syntax_item>
    = ID '=' <syntax_elements> <optional_class_restrictions>
    %%
	%<syntax_item>.stx = newstx(`&%'='.srcp,
				  %ID.id,
				  concat(%<syntax_elements>.elms,
				         newelm(`&%ID.id->srcp, ELMEOS,
				                NULL,
				                FALSE),
				         ELMNOD),
				  %<optional_class_restrictions>.ress);
    %%
    ;

<syntax_elements>
    = <syntax_element>
    %%
	%<syntax_elements>.elms = concat(NULL, %<syntax_element>.elm, ELMNOD);
    %%

    ! <syntax_elements> <syntax_element>
    %%
	%<syntax_elements>.elms = concat(%<syntax_elements>.elms,
				       %<syntax_element>.elm,
	                                ELMNOD);
    %%
    ;

<syntax_element>
    = ID
    %%
	%<syntax_element>.elm = newelm(`&%ID.id->srcp, ELMWRD,
				     %ID.id,
				     FALSE);
    %%

    ! '(' ID ')' <optional_indicators>

    %%
	%<syntax_element>.elm = newelm(`&%ID.id->srcp, ELMPAR,
				     %ID.id,
				     %<optional_indicators>.bits);
    %%
    ;

<optional_indicators>
    =
    %%
	%<optional_indicators>.bits = 0;
    %%

    ! <optional_indicators> <indicator>
    %%
	%<optional_indicators>.bits |= %<indicator>.bits;
    %%
    ;

<indicator>
    = '*'
    %%
	%<indicator>.bits = 0x1;
    %%

    ! '!'
    %%
	%<indicator>.bits = 0x2;
    %%
    ;

<optional_class_restrictions>
    = '.'
    %%
	%<optional_class_restrictions>.ress = NULL;
    %%

    ! 'WHERE' <class_restriction_clauses>
    %%
	%<optional_class_restrictions>.ress = %<class_restriction_clauses>.ress;
    %%
    ;


-- A class restriction is a definition of which classes a syntax parameter may belong to
-- A parameter may not be restricted in more than one restriction clause so there can be
-- at most as many restriction clauses as there are parameters. However a parameter can be
-- possibly be restricted to a list of classes in the same restriction clause.
<class_restriction_clauses>
    = <class_restriction>
    %%
	%<class_restriction_clauses>.ress = concat(NULL, %<class_restriction>.res, RESNOD);
    %%

    ! <class_restriction_clauses> 'AND' <class_restriction>
    %%
	%<class_restriction_clauses>.ress = concat(%<class_restriction_clauses>.ress,
				       %<class_restriction>.res, RESNOD);
    %%
    ;

<class_restriction>
    = ID 'ISA' <classes> 'ELSE' <statements>		%-('QUIT', 'SAVE', 'RESTORE', 'EMPTY', 'RESTART')
    %%
	%<class_restriction>.res = newres(`&%'ISA'.srcp,
			  		%ID.id,
				    	%<classes>.single,
				    	%<classes>.resKd,
					%<classes>.idList,
				    	%<statements>.stms);
    %%
    ;

<classes>			-- RETURNS: single, resKd, idList
    = <class_id>
    %%
	%<classes>.resKd = %<class_id>.resKd;
        %<classes>.single = TRUE;
	%<classes>.idList = concat(NULL, %<class_id>.id, IDNOD);
    %%

    ! <classes> 'OR' <class_id>
-- FIXME! Must be expressions instead of bit patterns, or possibly prohibited
    %%
    	unimpl(`&%'OR'.srcp, "Parsing");
        %<classes>.single = FALSE;
    %%
    ;

<class_id>			-- RETURNS: resKd, id
   = <heritage_id>
    %%
	if (strcmp(%<class_id>.id->str, "actor") == 0)
	    %<class_id>.resKd = ACTOR_RESTRICTION;
	else if (strcmp(%<class_id>.id->str, "object") == 0)
	    %<class_id>.resKd = OBJECT_RESTRICTION;
	else
	    %<class_id>.resKd = ID_RESTRICTION;
	%<class_id>.id = %<heritage_id>.id;
    %%

    ! 'CONTAINER' 'OBJECT'
    %%
	%<class_id>.resKd = CONTAINEROBJECT_RESTRICTION;
	%<class_id>.id = NULL;
    %%

    ! 'CONTAINER' 'ACTOR'
    %%
	%<class_id>.resKd = CONTAINERACTOR_RESTRICTION;
	%<class_id>.id = NULL;
    %%

    ! 'INTEGER'
    %%
	%<class_id>.resKd = INTEGER_RESTRICTION;
	%<class_id>.id = NULL;
    %%

    ! 'STRING'
    %%
	%<class_id>.resKd = STRING_RESTRICTION;
	%<class_id>.id = NULL;
    %%
    ;    


---> VERBS
<verb>			-- RETURNS: vrb
    = <verb_header> <verb_body> <verb_tail>
    %%
	%<verb>.vrb = newvrb(`&%<verb_header>.srcp,
			   %<verb_header>.idList,
			   %<verb_body>.alts);
	if (%<verb_tail>.id != NULL) { /* END-id given */
	    if (strcmp(%<verb_header>.str, %<verb_tail>.id->str) != 0)
		lmLog(`&%<verb_tail>.id->srcp, 201, sevWAR, %<verb_header>.str);
	}
    %%
    ;

<verb_header>		-- RETURNS: srcp, str, idList
    = 'VERB' <id_list>
    %%
	%<verb_header>.srcp   = %'VERB'.srcp;
	%<verb_header>.str    = %<id_list>.str;
	%<verb_header>.idList   = %<id_list>.idList;
    %%
    ;

<verb_body>
    = <simple_verb_body>
    %%
	%<verb_body>.alts = concat(NULL,
				 newalt(`&nulsrcp,
					NULL,
					%<simple_verb_body>.chks,
					%<simple_verb_body>.qual,
					%<simple_verb_body>.stms),
				   ALTNOD);
    %%

    ! <verb_alternatives>
    %%
	%<verb_body>.alts = %<verb_alternatives>.alts;
    %%
    ;
<verb_alternatives>
    = <verb_alternative>
    %%
	%<verb_alternatives>.alts = concat(NULL, %<verb_alternative>.alt, ALTNOD);
    %%

    ! <verb_alternatives> <verb_alternative>
    %%
	%<verb_alternatives>.alts = concat(%<verb_alternatives>.alts,
					 %<verb_alternative>.alt,
	     				ALTNOD);
    %%
    ;

<verb_alternative>
    = 'WHEN' ID <simple_verb_body>
    %%
	%<verb_alternative>.alt = newalt(`&%'WHEN'.srcp,
				       %ID.id,
				       %<simple_verb_body>.chks,
				       %<simple_verb_body>.qual,
				       %<simple_verb_body>.stms);
    %%
    ;

<simple_verb_body>		-- RETURNS: chks, stms, qual
    = <optional_checks> <optional_does>
    %%
	%<simple_verb_body>.chks = %<optional_checks>.chks;
	%<simple_verb_body>.qual = %<optional_does>.qual;
	%<simple_verb_body>.stms = %<optional_does>.stms;
    %%
    ;

<verb_tail>		-- RETURNS: nam
    = 'END' 'VERB' <optional_id> '.'
    %%
	%<verb_tail>.id = %<optional_id>.id;
    %%
    ;

----> CHECKS
<optional_checks>		-- RETURNS: chks
    =
    %%
	%<optional_checks>.chks = NULL;
    %%

    ! 'CHECK' <statements>
    %%
	%<optional_checks>.chks = concat(NULL, newchk(NULL, %<statements>.stms), STMNOD);
    %%

    ! 'CHECK' <check_list>
    %%
	%<optional_checks>.chks = %<check_list>.chks;
    %%
    ;

<check_list>		-- RETURNS: chks
    = <check>
    %%
	%<check_list>.chks = concat(NULL, %<check>.chk, CHKNOD);
    %%

    ! <check_list> 'AND' <check>
    %%
	%<check_list>.chks = concat(%<check_list>.chks, %<check>.chk, CHKNOD);
    %%
    ;

<check>			-- RETURNS: chk
    = <expression> 'ELSE' <statements>
    %%
	%<check>.chk = newchk(%<expression>.exp, %<statements>.stms);
    %%
    ;

----> DOES
<optional_does>		-- RETURNS: qual, srcp, stms
    =
    %%
	%<optional_does>.qual = QUAL_DEFAULT;
	%<optional_does>.stms = NULL;
    %%

    ! <does>
    -- Propagate!
    ;

<does>
    = 'DOES' <optional_qual> <statements>
    %%
	%<does>.qual = %<optional_qual>.qual;
	%<does>.srcp = %'DOES'.srcp;
	%<does>.stms = %<statements>.stms;
    %%
    ;


---> LOCATION
<location>		-- RETURNS: loc
    = <location_header> <location_body> <location_tail>
    %%
	%<location>.loc = newloc(`&%<location_header>.srcp,
			       %<location_header>.id,
			       %<location_header>.nam,
			       %<location_body>.atrs,
			       %<location_body>.stms,
			       %<location_body>.stms2,
			       %<location_body>.exts,
			       %<location_body>.vrbs);
	if (%<location_tail>.id != NULL) { /* END-id given */
	    if (!eqnams(%<location_header>.id, %<location_tail>.id))
		lmLog(`&%<location_tail>.id->srcp, 201, sevWAR,
			%<location_header>.id->str);
	}
    %%
    ;


<location_header>		-- RETURNS: srcp, id, nams
    = 'LOCATION' ID <optional_name>
    %%
	lmLog(`&%'LOCATION'.srcp, 800, sevWAR, "THE <id> ISA LOCATION ---");
	%<location_header>.srcp = %'LOCATION'.srcp;
	%<location_header>.id  = %ID.id;
	%<location_header>.nam = %<optional_name>.nam;
    %%
    ;

<location_body>		-- RETURNS: atrs, stms, stms2, exts, vrbs
    =
    %%
	%<location_body>.atrs  = NULL;
	%<location_body>.stms  = NULL;
	%<location_body>.stms2 = NULL;
	%<location_body>.exts  = NULL;
	%<location_body>.vrbs  = NULL;
    %%

    ! <location_body> <location_body_part>
    %%
      if (%<location_body_part>.atrs != NULL)
	%<location_body>.atrs = combine(%<location_body>.atrs, %<location_body_part>.atrs);
      else if (%<location_body_part>.stms != NULL)
	if (%<location_body>.stms != NULL)
          /* DESCRIPTION Already declared */
	  lmLogv(`&%<location_body_part>.srcp, 204, sevERR, "DESCRIPTION", "location", NULL);
        else
	  %<location_body>.stms = %<location_body_part>.stms;
      else if (%<location_body_part>.does != NULL)
	if (%<location_body>.does != NULL)
          /* DOES Already declared */
	  lmLogv(`&%<location_body_part>.srcp, 204, sevERR, "DOES", "location", NULL);
        else {
	  %<location_body>.does = %<location_body_part>.does;
	  if (%<location_body_part>.qual != QUAL_DEFAULT)

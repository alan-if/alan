	%<object_header>.srcp = %'OBJECT'.srcp;
	%<object_header>.id  = %ID.id;
	%<object_header>.nams = %<optional_names>.nams;
        if (%1<optional_where>.whr->whr != WHR_DEFAULT) {
          %<object_header>.whr  = %1<optional_where>.whr;
          if (%2<optional_where>.whr->whr != WHR_DEFAULT)
            lmLog(`&%2<optional_where>.whr->srcp, 340, sevWAR, %ID.id->str);

optional_exit_body	-- RETURNS: chks, stms, nam
    =
    %%
	%optional_exit_body.chks = NULL;
	%optional_exit_body.stms = NULL;
	%optional_exit_body.nam	 = NULL;
    %%

    ! optional_checks optional_does 'END' 'EXIT' optional_id

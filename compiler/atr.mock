/* Create a new Attribute node */
Attribute *newBooleanAttribute(Srcp srcp, IdNode *id, Bool value) { Attribute *atr = NEW(Attribute);; atr->srcp = srcp; atr->id = id; atr->value = value; return atr; }
Attribute *newStringAttribute(Srcp srcp, IdNode *id, long fpos, int len) { Attribute *atr = NEW(Attribute); atr->srcp = srcp; atr->id = id; atr->fpos = fpos; atr->len = len; return atr; }
Attribute *newIntegerAttribute(Srcp srcp, IdNode *id, int value)  { Attribute *atr = NEW(Attribute); atr->srcp = srcp; atr->id = id; atr->value = value; return atr; }
Attribute *newReferenceAttribute(Srcp srcp, IdNode *id, IdNode *instance)  { Attribute *atr = NEW(Attribute); atr->srcp = srcp; atr->id = id; atr->id = instance; return atr; }
Attribute *newSetAttribute(Srcp srcp, IdNode *id, Expression *set)  { Attribute *atr = NEW(Attribute); atr->srcp = srcp; atr->id = id; atr->set = set; return atr; }

void symbolizeAttributes(List *attributeList, Bool inClassDeclaration) { mock(attributeList, inClassDeclaration); }

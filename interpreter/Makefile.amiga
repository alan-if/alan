# Makefile for arun on the AMIGA	      Date: 1989-12-30/thoni@eeyore
#


CFLAGS = -n +l $(CQ)
LDFLAGS	= -lc32 -g $(LQ)

OBJECTS = arun.o \
	debug.o \
	exe.o \
	inter.o \
	parse.o \
	rules.o \
	stack.o \
	decode.o \
	term.o \
	params.o \
	reverse.o


#-- ARUN
arun: $(OBJECTS) sysdep.o version.o
	ln -o $@ $(OBJECTS) sysdep.o version.o $(LDFLAGS)
	copy arun /

arun.o: arun.c
	cc $(CFLAGS) -e400 arun.c

term.o: term.c
	cc $(CFLAGS) -L 1000 term.c

#-- ARCHIVE
archive:
	zoo -add interpreter *.c *.h Makefile.amiga


#---<< GENERATED DEPENDENCIES FOLLOWS >>---

SYNONYMS
    q = quit.

SYNTAX
    quit = quit.

VERB quit
    DOES
    	QUIT.
END VERB.


SYNTAX
    save = save.

VERB save
    DOES
	SAVE.
	"Done."
END VERB.


SYNTAX
    restore = restore.

VERB restore
    DOES
    	RESTORE.
    	"Done.$n"
	LOOK.
END VERB.


SYNTAX
    'score' = 'score'.

VERB 'score'
    DOES
    	SCORE.
END VERB 'score'.


SYNONYMS
    z = wait.

SYNTAX
    wait = wait.

VERB wait
    DOES
    	"Time passes..."
END VERB.


actor attributes
	a.

object attributes
	b.

actor x name mr x at l
end actor x.

object o at l
end object o.

location l
end location l.

start at l.
	if x is a then
		"X is a."
	else
		"X isn't a."
	end if.
	if o is b then
		"O is b."
	else
		"O isn't b."
	end if.
	if actor is a then
		"Actor is a."
	else
		"Actor isn't a."
	end if.
quit.
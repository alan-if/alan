options
  debug.


location l
end location.

object a at l
end object a.

object b at l
end object b.

syntax eq = 'eq' (a) (b).
verb eq
  does
    if a = b then
      "Same."
    else
      "Different."
    end if.
end verb.

start at l.
    if a = b then
      "Same."
    else
      "Different."
    end if.

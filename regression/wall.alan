OBJECT common_wall NAME wall

    VERB examine
	DOES
	    DESCRIBE common_wall.
    END VERB.

END object.

OBJECT wall NAME wall AT loc1

    VERB examine
	DOES
	    DESCRIBE wall.
    END VERB.

END OBJECT.

LOCATION loc1
  Description
    "e to loc2."
  Exit e to loc2.
END LOCATION.

LOCATION loc2
  Description
    "w to loc1."
  Exit w to loc1.
END LOCATION.

START AT loc1.
